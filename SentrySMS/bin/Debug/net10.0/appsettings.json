{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GsmSettings": {
    "SerialPort": "COM3",
    "BaudRate": 9600,
    "DataBits": 8,
    "Parity": "None",
    "StopBits": "One",
    "Handshake": "None",
    "ReadTimeout": 2000,
    "WriteTimeout": 2000,
    "NewLine": "\r\n"
  }
}
